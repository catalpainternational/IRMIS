{
    "name": "irmis",
    "version": "1.0.0",
    "description": "Integrated Roads Management Information System",
    "main": "index.js",
    "repository": "https://github.com/catalpainternational/roads",
    "author": "Catalpa International",
    "license": "MIT",
    "scripts": {
        "eslint": "tslint src/**/*.ts src/**/*.js src/**/*.html",
        "tslint": "tslint src/**/*.ts src/**/*.js src/**/*.html",
        "start": "webpack --watch --config webpack.dev.js",
        "build": "webpack --config webpack.prod.js",
        "dev": "webpack --config webpack.dev.js",
        "protoc_py": "protoc -I=./protobuf --python_out=./protobuf ./protobuf/roads.proto",
        "protoc_js": "protoc -I=./protobuf --js_out=import_style=commonjs,binary:./protobuf ./protobuf/roads.proto",
        "protoc": "yarn protoc_py && yarn protoc_js",
        "collect_gettext": "python riot_gettext.py src/riot/* > gettext_collected.js",
        "jsmessages": "yarn collect_gettext && python manage.py makemessages -d djangojs -i node_modules"
    },
    "devDependencies": {
        "@babel/core": "^7.5.5",
        "@babel/preset-env": "^7.5.5",
        "@riotjs/compiler": "^4.3.11",
        "@riotjs/webpack-loader": "^4.0.1",
        "babel-loader": "^8.0.6",
        "bootstrap": "^4.3.1",
        "clean-webpack-plugin": "^3.0.0",
        "css-loader": "^3.2.0",
        "cssnano": "^4.1.10",
        "extract-loader": "^3.1.0",
        "file-loader": "^4.2.0",
        "node-sass": "^4.12.0",
        "optimize-css-assets-webpack-plugin": "^5.0.3",
        "popper.js": "^1.15.0",
        "sass-loader": "^7.3.1",
        "svg-url-loader": "^3.0.0",
        "ts-loader": "^6.0.4",
        "tslint": "^5.18.0",
        "typescript": "^3.5.3",
        "url-loader": "^2.1.0",
        "webpack": "^4.39.3",
        "webpack-cli": "^3.3.7",
        "webpack-dev-server": "^3.8.0",
        "webpack-merge": "^4.2.2"
    },
    "dependencies": {
        "@types/leaflet": "^1.5.0",
        "@types/ua-parser-js": "^0.7.33",
        "babel-polyfill": "^6.26.0",
        "datatables.net": "^1.10.19",
        "datatables.net-bs4": "^1.10.19",
        "datatables.net-buttons-bs4": "^1.5.6",
        "geobuf": "^3.0.1",
        "google-protobuf": "^3.9.1",
        "jquery": "^3.4.1",
        "jszip": "^3.2.2",
        "leaflet": "^1.5.1",
        "riot": "^4.5.0",
        "riot-route": "^3.1.4",
        "select2": "^4.0.10",
        "ua-parser-js": "^0.7.20"
    }
}
