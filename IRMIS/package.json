		"datatables.net": "^1.10.19",
		"datatables.net-bs4": "^1.10.19",
		"datatables.net-select": "^1.3.0",
		"geobuf": "^3.0.1",
		"google-protobuf": "^3.9.1",
		"jquery": "^3.4.1",
		"leaflet": "^1.5.1",
		"select2": "^4.0.9",
		"ua-parser-js": "^0.7.20"
	}
