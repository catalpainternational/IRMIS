		"extract-loader": "^3.1.0",
		"file-loader": "^4.1.0",
		"node-sass": "^4.12.0",
		"optimize-css-assets-webpack-plugin": "^5.0.3",
		"popper.js": "^1.15.0",
		"sass-loader": "^7.1.0",
		"svg-url-loader": "^3.0.0",
		"ts-loader": "^6.0.4",
		"tslint": "^5.18.0",
		"typescript": "^3.5.3",
		"url-loader": "^2.1.0",
		"webpack": "^4.38.0",
