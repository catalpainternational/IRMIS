	"author": "Catalpa International",
	"license": "MIT",
	"scripts": {
		"eslint": "tslint src/**/*.ts src/**/*.js src/**/*.html",
		"tslint": "tslint src/**/*.ts src/**/*.js src/**/*.html",
		"start": "webpack --watch --config webpack.dev.js",
		"build": "webpack --config webpack.prod.js",
		"dev": "webpack --config webpack.dev.js",
		"protoc_py": "protoc -I=./protobuf --python_out=./protobuf ./protobuf/roads.proto",
		"protoc_js": "protoc -I=./protobuf --js_out=import_style=commonjs,binary:./protobuf ./protobuf/roads.proto",
		"protoc": "yarn protoc_py && yarn protoc_js"
	},
	"devDependencies": {
		"babel-core": "^6.26.3",
		"babel-loader": "^7.1.5",
